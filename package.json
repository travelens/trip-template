  "version": "2.0.0",
  "scripts": {
    "dev": "astro dev",
    "clean": "rm -rf node_modules .astro dist",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "check": "astro check",
    "lint": "pnpm run lint:prettier ; pnpm run lint:eslint",
    "lint:prettier": "prettier --write  \"src/**/*.{js,jsx,ts,tsx,md,mdx,astro}\"",
    "lint:eslint": "eslint --fix \"src/**/*.{js,ts,jsx,tsx,astro}\"",
    "astro": "astro",
    "copydocs": "cp dist/documents.json ../travelens.github.io/src/data/${npm_package_name}.json",
    "upgrade": "pnpm dlx @astrojs/upgrade; pnpm up -L"
  },
  "dependencies": {
    "@astrojs/alpinejs": "^0.4.3",
    "@astrojs/markdoc": "^0.12.10",
