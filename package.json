    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-markdown": "^5.1.0",
    "prettier": "^3.5.2",
    "prettier-config-standard": "^7.0.0",
    "prettier-plugin-astro": "^0.14.1",
    "schema-dts": "^1.1.2",
    "sharp": "^0.33.5",
    "unocss": "66.1.0-beta.3"
